filetype plugin indent on
syntax enable
set nocompatible
set ruler
set number
set relativenumber
set background=dark
set incsearch
set hlsearch
set mouse=a
set backspace=indent,eol,start
set smartindent
set wildmenu wildmode=longest,list
set hidden
set expandtab
set tabstop=4
set shiftwidth=4
set softtabstop=4
set nobackup
set nowb
set noswapfile
map <C-n> :tabn<CR>
map <C-b> :tabp<CR>
map <C-e> :tabe
