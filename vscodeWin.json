// Place your settings in this file to overwrite the default settings
{
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [80],
    "vim.useSystemClipboard": true,
    "git.autorefresh": false,
    "window.zoomLevel": 0,
    "files.associations": {
        "*.stfu": "json"
    },
    "vim.disableAnnoyingNeovimMessage": true,
    "vim.handleKeys": {
        "<C-f>": false,
        "<C-w>": false
    }
}
